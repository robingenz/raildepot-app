{
  "name": "angular-sqlite-starter",
  "version": "2.1.1",
  "description": "The Ionic Starter to launch apps in record time.",
  "author": "Robin Genz",
  "homepage": "https://ionstarter.dev/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run sync:environment && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --configuration ci",
    "lint": "ng lint && npm run prettier -- --check",
    "fmt": "ng lint --fix && npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{css,scss,html,ts,js}\"",
    "release": "commit-and-tag-version --commit-all",
    "setup": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./scripts/setup.mts",
    "sync:environment": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./scripts/sync-environment.mts",
    "sync:native-version": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./scripts/sync-native-versions.mts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.2.1",
    "@angular/common": "17.2.1",
    "@angular/core": "17.2.1",
    "@angular/forms": "17.2.1",
    "@angular/platform-browser": "17.2.1",
    "@angular/platform-browser-dynamic": "17.2.1",
    "@angular/router": "17.2.1",
    "@capacitor-community/sqlite": "6.0.0-alpha.1",
    "@capacitor/android": "6.0.0",
    "@capacitor/app": "6.0.0",
    "@capacitor/browser": "6.0.0",
    "@capacitor/core": "6.0.0",
    "@capacitor/filesystem": "6.0.0",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/ios": "6.0.0",
    "@capacitor/keyboard": "6.0.0",
    "@capacitor/preferences": "6.0.0",
    "@capacitor/splash-screen": "6.0.0",
    "@capacitor/status-bar": "6.0.0",
    "@capawesome/capacitor-live-update": "6.0.7",
    "@ionic/angular": "8.2.0",
    "@ionic/core": "8.2.0",
    "@jsverse/transloco": "7.4.0",
    "@jsverse/transloco-persist-lang": "7.0.0",
    "@jsverse/transloco-persist-translations": "7.0.0",
    "@ngneat/elf": "2.5.1",
    "@ngneat/elf-cli-ng": "1.0.0",
    "@ngneat/elf-devtools": "1.3.0",
    "@ngneat/elf-entities": "5.0.1",
    "@ngneat/elf-persist-state": "1.2.1",
    "@ngneat/query": "2.0.1",
    "@revenuecat/purchases-capacitor": "8.0.0-beta.1",
    "@tanstack/query-core": "5.24.6",
    "ionicons": "7.2.2",
    "nanoid": "5.0.6",
    "rxjs": "7.8.1",
    "tslib": "2.3.0",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "17.2.0",
    "@angular/compiler": "17.2.1",
    "@angular/compiler-cli": "17.2.1",
    "@angular/language-service": "17.2.1",
    "@capacitor/cli": "6.0.0",
    "@capawesome/cli": "0.0.7",
    "@ionic/angular-toolkit": "11.0.1",
    "@ionic/cli": "7.2.0",
    "@trapezedev/project": "7.0.10",
    "@types/inquirer": "9.0.7",
    "@types/jasmine": "4.3.5",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "commit-and-tag-version": "12.2.0",
    "eslint": "8.56.0",
    "eslint-plugin-unicorn": "51.0.1",
    "inquirer": "9.2.15",
    "jasmine-core": "4.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "rimraf": "5.0.5",
    "semver": "7.6.0",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ],
      "repoLibrary": "src/app/store"
    }
  },
  "commit-and-tag-version": {
    "scripts": {
      "postbump": "npm run sync:native-version && git add android/app/build.gradle ios/App/App.xcodeproj/project.pbxproj"
    }
  }
}
